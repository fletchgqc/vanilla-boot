{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:best-practices"
    ],
    "automerge": true,
    "packageRules": [
        {
            "groupName": "all dependencies",
            "matchPackageNames": [
                "*"
            ]
        }
    ],
    "rebaseWhen": "behind-base-branch"
}
